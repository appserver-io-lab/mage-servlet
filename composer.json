    "keywords": [ "servlet", "web", "container", "appserver" ],
    "license": [ "OSL-3.0" ],
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "tw@appserver.io",
            "homepage": "http://www.appserver.io",
